# Template for Gemini API usage
GEMINI_API_KEY=your_api_key_here

GEMINI_CACHE_TTL_SECONDS=3600

ENABLE_PARALLELISM=0

LOG_LEVEL=INFO

AUTOMATED_AGENT_MAX_EXCHANGES=8

COLLAPSE_PROMPT_SECTIONS_IN_DEBUG_LOGS=1

# SQLite logging (evaluations + web)
ENABLE_SQLITE_LOGGING=0
EVALUATION_SQLITE_PATH=/var/lib/sqlite/main.db
LOG_WEB_RUNS_TO_DB=0
WEB_DB_NOTES=
